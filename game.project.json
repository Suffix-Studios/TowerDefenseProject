{
    "name": "roblox-ts-game",
	"globIgnorePaths": [
        "**/package.json",
		"**/tsconfig.json"
	],
    "emitLegacyScripts": false,
	"tree": {
		"$className": "DataModel",
		"ServerScriptService": {
			"$className": "ServerScriptService",
            "$path": "out/Server"
		},

        "ReplicatedFirst": {
            "$className": "ReplicatedFirst",
            "$path": "out/First"
        },

		"ReplicatedStorage": {
			"$className": "ReplicatedStorage",
			"rbxts_include": {
				"$path": "include",
				"node_modules": {
					"$className": "Folder",
					"@rbxts": {
						"$path": "node_modules/@rbxts"
					}
				}
			},

			"Modules": {
                "$className": "Folder",
				"$path": "out/Shared"
			},

            "Client": {
                "$className": "Folder",
                "$path": "out/Client",

				"UI": {
					"$className": "Folder",
					"$path": "out/UI"
				}
            }
		},

		"StarterPlayer": {
			"$className": "StarterPlayer",
            "StarterCharacterScripts": {
                "$className": "StarterCharacterScripts",
                "$path": "out/Character"
            }
		}
	}
}
